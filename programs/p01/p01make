#------------------------------------------------------------------------------
#Object Files
#------------------------------------------------------------------------------
obj	= p01.o List01.o
#------------------------------------------------------------------------------
# Create Executable File p01
#------------------------------------------------------------------------------
p01:	${obj}
		g++ -o p01 ${obj} -lm
#------------------------------------------------------------------------------
# Compile p01.cpp
#------------------------------------------------------------------------------
p01.o:	p01.cpp List01.h
		g++ -c -g p01.cpp
#------------------------------------------------------------------------------
# Compile List01.cpp
#------------------------------------------------------------------------------
List01.o: List01.cpp List01.h
			g++ -c -g List01.cpp
